ntainer, camera, renderer) {\n    // set initial size on load\n    setSize(container, camera, renderer);\n\n    window.addEventListener('resize', () => {\n      // set the size again if a resize occurs\n      setSize(container, camera, renderer);\n      // perform any custom actions\n      this.onResize();\n    });\n  }\n\n  onResize() {}\n}\n\nexport { Resizer };"]}]}